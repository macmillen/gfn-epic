---
import GameTable from "../../components/game-table.astro";
import _data from "../../data/generated/4-final-data.json";
import Layout from "../../layouts/layout.astro";

const data = _data.filter((v) => v.gfnUrl);
---

<Layout
  title="GFN EPIC | only GFN supported games"
  gfnOnly
  data={data}
  metaDescription="A list of all free Epic Games which are supported by GeforceNow."
>
  <GameTable data={data} />
</Layout>
